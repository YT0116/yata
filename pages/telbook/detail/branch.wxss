/* pages/telbook/detail/branch.wxss */
page {
  background-color: #F5F5F5;
}
.fill-whole{
  display: flex;
  height: 100%;
  flex-direction: column;
  margin: auto;
  width: 100%;
  text-align: center;
  align-items: center;
}
 .tr{
  background-color: white;
  display: flex;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  vertical-align: middle;
  text-align: left; 
}
.td{
  font-size: 28rpx;
  line-height: 90rpx;
  vertical-align: middle;
  margin-left: 7%;
  width: 80rpx;
  text-align: left;

} 

.text{
  margin-left: 20rpx;
  font-size: 28rpx;
  font-weight: 200;
  width: 70%;
}

.img{
  margin-left: 7%;
  width: 80rpx;
}
.uimg{
  margin-left: 7%;
  width: 40rpx;
}
.img-arrow{
  position: absolute; 
  margin-top: 65rpx;
  right: 8%;
  width: 30rpx;
}
.data{
  margin-left: 20rpx;
  font-size: 28rpx;
  font-weight: 200rpx;
  width: 200rpx;
  display:flex;
  flex-direction: row;  

}

.btn-area{
  width: 100%;
  display: flex;
  flex-direction: row;
  bottom: 0;
  position: fixed;
  margin-bottom:50rpx;
  justify-content: space-around;
}

.btn{
  width: 250rpx;
  border-radius: 50rpx;
  border: solid 1rpx #dcdcdc
}

.tag{
  position:absolute;
  margin-left: 13%; 
  margin-top: 25rpx;
  width: 125rpx;
  height: 40rpx;
  line-height: 40rpx;
  vertical-align: middle;
  text-align: center;
  font-size: 26rpx;
  color: white;
  border-radius: 25rpx; 
}

