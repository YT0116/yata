/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.appwhole{
  display: flex;
  height: 100%;
  flex-direction: column;
  margin: auto;
  width: 85%;
  align-items: center;
}
/*////////            版权声明            ///////*/
.claim{
  display: flex;
  width: 100%;
  flex-direction: column;
  align-self: center;
  text-align: center;
  position: absolute;
  bottom: 50rpx;
  color: gray;
  font-size: 12px;
}
/*////////            分割线            ///////*/
.divline{
  height: 1rpx;
  width: 100%;
  border-bottom: 1px #F5F5F5 solid;
}